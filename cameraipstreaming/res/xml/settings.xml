<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <PreferenceCategory
        android:title="@string/category_main">

        <ListPreference
            android:key="device"
            android:title="@string/policy_label"
            android:defaultValue="POLICY_SUBJECT_WARNING"
            android:entries="@array/policyLabels"
            android:entryValues="@array/policyValues"
            />

        <EditTextPreference
            android:key="url"
            android:title="@string/password_label"
            android:defaultValue=""
            />
        <ListPreference
            android:key="environments"
            android:title="@string/eviron"
            android:defaultValue="POLICY_SUBJECT_EVIRON"
            android:entries="@array/evironLabel"
            android:entryValues="@array/evironValues"
            />
       

    </PreferenceCategory>
    
</PreferenceScreen>
